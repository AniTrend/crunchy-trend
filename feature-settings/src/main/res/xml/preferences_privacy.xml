<androidx.preference.PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

    <SwitchPreferenceCompat
            app:defaultValue="false"
            app:icon="@drawable/ic_privacy_lock"
            app:key="@string/settings_privacy_usage_analytics"
            app:summary="@string/preference_summary_privacy_analytics_config"
            app:title="@string/preference_title_privacy_analytics_config" />

    <SwitchPreferenceCompat
            app:defaultValue="true"
            app:icon="@drawable/ic_privacy_bug"
            app:key="@string/settings_privacy_crash_analytics"
            app:summary="@string/preference_summary_privacy_crash_analytics_config"
            app:title="@string/preference_title_privacy_crash_analytics_config" />

</androidx.preference.PreferenceScreen>